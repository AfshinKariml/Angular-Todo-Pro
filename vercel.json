{
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|otf|map|json))$",
      "headers": { "cache-control": "max-age=31536000" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "node build.js && ng build --configuration production",
  "outputDirectory": "dist/todo-pro-application",
  "installCommand": "npm install",
  "framework": "angular"
}